<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.CarSensorDAO">
	
	<insert id="insert" parameterType="carsensor">
		INSERT INTO ADMIN VALUES (#{adminid},#{adminpwd},#{adminname},#{adminphone},#{adminstate})
	</insert>
	
	<update id="update" parameterType="carsensor">
		UPDATE ADMIN SET ADMINPWD=#{adminpwd}, ADMINNAME=#{adminname}, ADMINPHONE=#{adminphone}, ADMINSTATE=#{adminstate} WHERE ADMINID=#{adminid}
	</update>
	
	<delete id="delete" parameterType="Integer">
		DELETE FROM ADMIN WHERE ADMINID=#{k}
	</delete>
	
	<select id="select" parameterType="Integer" resultType="carsensor">
		SELECT * FROM ADMIN WHERE ADMINID=#{k}
	</select>
	
	<select id="selectall" resultType="carsensor">
		SELECT * FROM ADMIN 
	</select>
	
	
</mapper>